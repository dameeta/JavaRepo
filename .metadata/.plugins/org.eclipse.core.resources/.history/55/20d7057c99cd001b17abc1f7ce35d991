package ArraysExamples;

public class CharArray {

	public char[] createArray() {
		 char[] s;
		 s = new char[26];
		 for ( int i=0; i<26; i++ ) {
		 s[i] = (char) ('A' + i);
		 }
		
		 return s;
		 }
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		CharArray cha=new CharArray();
		;
		System.out.println(cha.createArray());
		System.out.print(" ");
	}

}
