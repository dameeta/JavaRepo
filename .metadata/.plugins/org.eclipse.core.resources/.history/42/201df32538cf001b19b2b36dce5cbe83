
public class finallyDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int x=6 ,y=1;
		try {
			System.out.println(x/y);
		}
		catch(ArithmeticException e1)
		{
			
		}
		catch(Exception e)
		{
			
			
		}
		finally
		{
			System.out.println("Always execute..");
			
		}	
	}
	

}
