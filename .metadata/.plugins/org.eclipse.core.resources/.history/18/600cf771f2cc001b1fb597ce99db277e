package ArraysExamples;

public class OneDim {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int x=5;
		int x1 [];
		x1 = new int[3];
		x1[0] = 11;
		x1[1] =12;
		x1[2] =13;
		
		for(int i=0 ; i<=2 ;i++)
		{
			System.out.print(x1[i]);
			System.out.print(" ");
			
		}
		
		
	}

}
